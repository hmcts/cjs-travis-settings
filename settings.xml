<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">
    <interactiveMode>false</interactiveMode>
    <servers>
        <!-- for cloudsmith -->
        <server>
            <id>cjscommonplatform-maven-repository</id>
            <username>token</username>
            <password>${env.CLOUDSMITH_API_KEY}</password>
            <configuration>
                <httpConfiguration>
                    <all>
                        <connectionTimeout>20000</connectionTimeout> <!-- 20 seconds --> 
                        <readTimeout>120000</readTimeout> <!-- 120 seconds -->
                    </all>
                </httpConfiguration>
            </configuration>
        </server>
        <server>
            <id>jitpack.io</id>
            <configuration>
                <httpConfiguration>
                    <all>
                        <connectionTimeout>20000</connectionTimeout> <!-- 20 seconds -->
                        <readTimeout>120000</readTimeout> <!-- 120 seconds -->
                    </all>
                </httpConfiguration>
            </configuration>
        </server>
<!--        <server>-->
<!--            <id>central</id>-->
<!--            <configuration>-->
<!--                <httpConfiguration>-->
<!--                    <all>-->
<!--                        <connectionTimeout>20000</connectionTimeout> &lt;!&ndash; 20 seconds &ndash;&gt;-->
<!--                        <readTimeout>900000</readTimeout> &lt;!&ndash; 120 seconds &ndash;&gt;-->
<!--                    </all>-->
<!--                </httpConfiguration>-->
<!--            </configuration>-->
<!--        </server>-->
    </servers>
